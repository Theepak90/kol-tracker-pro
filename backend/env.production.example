# Production Environment Variables for Render
NODE_ENV=production
PORT=3000

# MongoDB Atlas Connection (will be set in Render dashboard)
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/kol_tracker?retryWrites=true&w=majority

# JWT Secret (will be auto-generated in Render)
JWT_SECRET=your-super-secret-jwt-key-here

# Telethon Service URL
TELETHON_SERVICE_URL=https://kol-tracker-telethon.onrender.com

# CORS Origins
CORS_ORIGINS=https://kol-tracker-frontend.onrender.com,https://kol-tracker-pro.netlify.app

# API Configuration
API_PREFIX=api
API_VERSION=v1

# Logging
LOG_LEVEL=info 